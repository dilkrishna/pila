{% extends "::base.html.twig" %}

{% block head_style %}
	{{ parent() }}
	{% block structured_document_head_style %}
	{% endblock %}

{% endblock head_style %}

{% block head_script %}
	{{ parent() }}
		{% block structured_document_head_script %}
			<script>
				var structured_document = {
					 locale: "{{ app.request.locale }}"
				};
			</script>
		{% endblock %}
{% endblock head_script %}

{% block page_class -%}{% block structured_document_page_class %}{% endblock %}{%- endblock %}

{% block page_title %}
	{% block structured_document_page_title %}{% endblock %}
{% endblock %}

{% block content %}
	{% block structured_document_content %}{% endblock %}
{% endblock %}

{% block foot_script %}
	{{ parent() }}
	{% block structured_document_foot_script %}{% endblock %}
{% endblock foot_script %}